{
  "name": "value-search",
  "version": "1.0.0",
  "description": "A template used as a starter application for create MERN applications",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nick-ramsay/value-search.git"
  },
  "author": "Nick Ramsay",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nick-ramsay/value-search/issues"
  },
  "homepage": "https://github.com/nick-ramsay/value-search-ai#readme",
  "dependencies": {
    "axios": "^1.12.2",
    "body-parser": "^2.2.0",
    "concurrently": "^9.2.1",
    "dd-trace": "^5.67.0",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "googleapis": "^160.0.0",
    "if-env": "^1.0.4",
    "mongodb": "^6.20.0",
    "mongoose": "^8.18.2",
    "nodemailer": "^7.0.6",
    "nodemon": "^3.1.10",
    "sha256": "^0.2.0"
  }
}
